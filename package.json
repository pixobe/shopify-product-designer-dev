    "@prisma/client": "^7.1.0",
    "@react-router/dev": "^7.10.1",
    "@react-router/fs-routes": "^7.10.1",
    "@react-router/node": "^7.10.1",
    "@react-router/serve": "^7.10.1",
    "@shopify/app-bridge-react": "^4.2.8",
    "@shopify/shopify-app-react-router": "^1.0.3",
    "@shopify/shopify-app-session-storage-prisma": "^7.0.3",
    "isbot": "^5.1.32",
    "prisma": "^7.1.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-router": "^7.10.1",
